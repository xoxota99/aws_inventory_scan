{
  "aws": {
    "default_region": "us-east-1",
    "global_services": [
      "iam",
      "s3",
      "route53",
      "cloudfront",
      "organizations",
      "waf",
      "shield",
      "budgets",
      "ce",
      "chatbot",
      "health"
    ],
    "default_services": [
      "ec2",
      "s3",
      "lambda",
      "dynamodb",
      "rds",
      "iam",
      "cloudformation",
      "sqs",
      "sns",
      "kinesisanalytics",
      "kinesisanalyticsv2",
      "cloudwatch",
      "logs",
      "route53",
      "ecs",
      "kms"
    ],
    "max_threads": 5,
    "max_retries": 5,
    "initial_backoff": 1,
    "max_backoff": 60
  },
  "output": {
    "default_output_file": "aws_resource_arns.json",
    "output_format": "json",
    "pretty_print": true
  },
  "logging": {
    "log_level": "INFO",
    "log_file": "",
    "log_format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
  },
  "scan": {
    "include_objects": true,
    "max_objects_per_bucket": 100,
    "scan_all_regions": true,
    "skip_empty_services": true
  }
}